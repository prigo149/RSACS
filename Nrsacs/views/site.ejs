<!DOCTYPE html>
<html lang="en">
<head>
    <% include head %>
</head>
<body class="container">

<header>
    <% include header %>
</header>

<main>
    <div class="jumbotron">
        <h1><%=title%></h1>
	    <ul>
		    <% records.forEach(function(site) { %>
    	      <li>
              <%  for(var property in site){ %>
                   <% if(site.hasOwnProperty(property)){ %>
                       <%= site[property] %> 
                    <%}%>
               <% }%>
              </li>
		    <% }); %>
		</ul>
    </div>
</main>

<footer>
    <% include footer %>
</footer>

</body>
</html>